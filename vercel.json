{
  "version": 2,
  "name": "careerpath-platform",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/fed.html"
    },
    {
      "src": "/jobs",
      "dest": "/jobs.html"
    },
    {
      "src": "/careers",
      "dest": "/css-careers.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}